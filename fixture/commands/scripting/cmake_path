# Decomposition
cmake_path(GET path_var1 ROOT_NAME out_var1)
cmake_path(GET path_var1 ROOT_DIRECTORY out_var1)
cmake_path(GET path_var1 ROOT_PATH out_var1)
cmake_path(GET path_var1 FILENAME out_var1)
cmake_path(GET path_var1 EXTENSION LAST_ONLY out_var1)
cmake_path(GET dotPath EXTENSION dotExt)
cmake_path(GET path_var1 STEM LAST_ONLY out_var1)
cmake_path(GET dotPath STEM dotStem)
cmake_path(GET path_var1 RELATIVE_PART out_var1)
cmake_path(GET path_var1 PARENT_PATH out_var1)

# Query
cmake_path(HAS_ROOT_NAME path_var1 out_var1)
cmake_path(HAS_ROOT_DIRECTORY path_var1 out_var1)
cmake_path(HAS_ROOT_PATH path_var1 out_var1)
cmake_path(HAS_FILENAME path_var1 out_var1)
cmake_path(HAS_EXTENSION path_var1 out_var1)
cmake_path(HAS_STEM path_var1 out_var1)
cmake_path(HAS_RELATIVE_PART path_var1 out_var1)
cmake_path(HAS_PARENT_PATH path_var1 out_var1)
cmake_path(IS_ABSOLUTE path_var1 out_var1)
cmake_path(IS_RELATIVE path_var1 out_var1)
cmake_path(IS_PREFIX path_var1 input1 out_var1)
cmake_path(IS_PREFIX path_var1 input1 NORMALIZE out_var1)
cmake_path(COMPARE input1 EQUAL input2 out_var1)
cmake_path(COMPARE input1 NOT_EQUAL input2 out_var1)

# Modification
cmake_path(SET path_var1 NORMALIZE input1)
cmake_path(SET path_var1 input1)
cmake_path(APPEND path_var1)
cmake_path(APPEND path_var1 input1 input2 input3)
cmake_path(APPEND path_var1 input1 input2 input3 OUTPUT_VARIABLE out_var1)
cmake_path(APPEND path_var1 OUTPUT_VARIABLE out_var1)
cmake_path(APPEND_STRING path_var1)
cmake_path(APPEND_STRING path_var1 input1 input2 input3)
cmake_path(APPEND_STRING path_var1 input1 input2 input3 OUTPUT_VARIABLE out_var1)
cmake_path(APPEND_STRING path_var1 OUTPUT_VARIABLE out_var1)
cmake_path(REMOVE_FILENAME path_var1)
cmake_path(REMOVE_FILENAME path_var1 OUTPUT_VARIABLE out_var1)
cmake_path(REPLACE_FILENAME path_var1 input1)
cmake_path(REPLACE_FILENAME path_var1 input1 OUTPUT_VARIABLE out_var1)
cmake_path(REMOVE_EXTENSION path_var1)
cmake_path(REMOVE_EXTENSION path_var1 OUTPUT_VARIABLE out_var1)
cmake_path(REMOVE_EXTENSION path_var1 LAST_ONLY)
cmake_path(REMOVE_EXTENSION path_var1 LAST_ONLY OUTPUT_VARIABLE out_var1)
cmake_path(REPLACE_EXTENSION path_var1 input1)
cmake_path(REPLACE_EXTENSION path_var1 input1 OUTPUT_VARIABLE out_var1)
cmake_path(REPLACE_EXTENSION path_var1 LAST_ONLY input1)
cmake_path(REPLACE_EXTENSION path_var1 LAST_ONLY input1 OUTPUT_VARIABLE out_var1)

# Generation
cmake_path(NORMAL_PATH path_var1)
cmake_path(NORMAL_PATH path_var1 OUTPUT_VARIABLE out_var1)
cmake_path(RELATIVE_PATH path_var1)
cmake_path(RELATIVE_PATH path_var1 BASE_DIRECTORY input1)
cmake_path(RELATIVE_PATH path_var1 OUTPUT_VARIABLE out_var1)
cmake_path(RELATIVE_PATH path_var1 BASE_DIRECTORY input1 OUTPUT_VARIABLE out_var1)
cmake_path(ABSOLUTE_PATH path_var1)
cmake_path(ABSOLUTE_PATH path_var1 BASE_DIRECTORY input1)
cmake_path(ABSOLUTE_PATH path_var1 OUTPUT_VARIABLE out_var1)
cmake_path(ABSOLUTE_PATH path_var1 BASE_DIRECTORY input1 OUTPUT_VARIABLE out_var1)
cmake_path(ABSOLUTE_PATH path_var1 BASE_DIRECTORY input1 NORMALIZE OUTPUT_VARIABLE out_var1)

# Native Conversion
cmake_path(NATIVE_PATH path_var1)
cmake_path(NATIVE_PATH path_var1 NORMALIZE out_var1)
cmake_path(CONVERT input1 TO_CMAKE_PATH_LIST out_var1)
cmake_path(CONVERT input1 TO_CMAKE_PATH_LIST out_var1 NORMALIZE)
cmake_path(CONVERT input1 TO_NATIVE_PATH_LIST out_var1)
cmake_path(CONVERT input1 TO_NATIVE_PATH_LIST out_var1 NORMALIZE)

# Hashing
cmake_path(HASH path_var1 out_var1)
