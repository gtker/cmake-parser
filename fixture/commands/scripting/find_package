    CONFIGS config1 config2
    HINTS hint1 hint2
    PATHS path1 path2
    REGISTRY_VIEW 64_32
    PATH_SUFFIXES suffix1 suffix2
    NO_DEFAULT_PATH
    NO_PACKAGE_ROOT_PATH
    NO_CMAKE_PATH
    NO_CMAKE_ENVIRONMENT_PATH
    NO_SYSTEM_ENVIRONMENT_PATH
