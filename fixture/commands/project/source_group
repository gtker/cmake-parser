source_group(grp1 regex1)
source_group(grp2 REGULAR_EXPRESSION regex2)
source_group(grp3)
source_group(grp4 FILES file1 file2)
source_group(grp5 FILES file3 file4 REGULAR_EXPRESSION regex3)
source_group(TREE grp6)
source_group(TREE grp7 PREFIX prefix1)
source_group(TREE grp8 FILES file5 file6)
source_group(TREE grp9 PREFIX prefix2 FILES file7 file8)
